sindico
id integer
nome string
| UM PARA MUITOS
edificio
id integer
nome string
endereco string
estrelas integer
  | lista
apartamento
id integer
medida string
financiado boolean
valorCOndominio bigdeciaml
numero string
piso string
 | lista
comodo
id integer
descricao string
medida string
possuiJanela boolean

CREATE TABLE SINDICO(
	SIN_ID INTEGER NOT NULL PRIMARY KEY,
	SIN_NOME VARCHAR(60)
)

CREATE TABLE EDIFICIO(
	EDI_ID INTEGER PRIMARY KEY,
	EDI_NOME VARCHAR(60),
	EDI_END VARCHAR(100),
	EDI_EST INTEGER,
	EDI_SIN INTEGER,
	CONSTRAINT EDIFICIO_SINDICO_PK
	FOREIGN KEY (EDI_SIN)
	REFERENCES SINDICO(SIN_ID)
)
CREATE TABLE APARTAMENTO(
	APA_ID INTEGER PRIMARY KEY,
	APA_MED VARCHAR(60),
	APA_FIN BOOLEAN,
	APA_VAL DECIMAL(6,2),
	APA_NUM VARCHAR(60),
	APA_PIS VARCHAR(60),
	APA_EDI INTEGER,
	CONSTRAINT APARTAMENTO_EDIFICIO_PK
	FOREIGN KEY(APA_EDI)
	REFERENCES EDIFICIO(EDI_ID)
)
CREATE TABLE COMODO(
	COM_ID INTEGER PRIMARY KEY,
	COM_DESC VARCHAR(60),
	COM_MED VARCHAR(60),
	COM_JAN BOOLEAN,
	COM_APA INTEGER,
	CONSTRAINT COMODO_APARTAMENTO_PK
	FOREIGN KEY(COM_APA)
	REFERENCES APARTAMENTO(APA_ID)
)